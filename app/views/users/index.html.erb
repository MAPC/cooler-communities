<h4><strong>Users Index</strong></h4>
<table style="width:100%">
  <tr>
    <th>id</th>
    <th>admin</th>
    <th>affiliation</th>
    <th>first_name</th>
    <th>last_name</th> 
    <th>username</th>
    <th>email</th>
    <th>phone</th>
    <th>address</th>
    <th>city</th>
    <th>state</th>
    <th>zipcode</th>
  </tr>
  
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id%></td>
      <td><%= user.admin%></td>
      <td><%= user.affiliation.name %></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.phone %></td>
      <td><%= user.address %></td>
      <td><%= user.city %></td>
      <td><%= user.state %></td>
      <td><%= user.zipcode %></td
    </tr>
  <% end %>
</table>
