<div class="main-nav">
  <div>
    <%= link_to (image_tag("cc-logo.png", class: "main-nav__cc-logo")), root_path %>
  </div>
  <div class="main-nav__links">
    <% if !current_user %>
    <%= link_to "Login", login_path, class: "main-nav__link" %>
    <span class="main-nav__link-separator"> | </span>
    <%= link_to "Register", register_path, class: "main-nav__link" %>
      <% else %>
      <%= link_to "Logout", destroy_user_session_path, class: "main-nav__link" %>
      <span class="main-nav__link-separator"> | </span>
      <%= link_to "Summary", user_summary_path, class: "main-nav__link" %>
    <% end %>
  </div>
</div>
